document.addEventListener('DOMContentLoaded', () => {
    if (annyang) {
        const commands = {
            'ban *username': banUser,
            'demote first admin': demoteFirstAdmin
        };

        annyang.addCommands(commands);
        annyang.start();

        console.log('Voice commands are ready.');
    }

