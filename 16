    normal_files = np.random.rand(5000, 3) * [100, 500, 10]
    malware_files = np.random.rand(5000, 3) * [1000, 5000, 100]

    data = np.concatenate([normal_files, malware_files])
    labels = np.concatenate([np.zeros(5000), np.ones(5000)])

    return data, labels

