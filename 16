    dns_spoofing_traffic = np.random.rand(5000, 3) * [10000, 5000, 100]

    data = np.concatenate([normal_traffic, dns_spoofing_traffic])
    labels = np.concatenate([np.zeros(5000), np.ones(5000)])

    return data, labels

